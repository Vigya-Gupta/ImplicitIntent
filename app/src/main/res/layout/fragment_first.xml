<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#232F34"
    tools:context=".FirstFragment">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardCall"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:clickable="true"
        android:foreground="@drawable/ic_baseline_call_24"
        app:cardBackgroundColor="#344955"
        app:cardCornerRadius="16dp"
        app:cardElevation="10dp"
        app:layout_constraintEnd_toStartOf="@+id/cardMsg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardMsg"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="20dp"
        android:clickable="true"
        android:foreground="@drawable/ic_baseline_message_24"
        app:cardBackgroundColor="#344955"
        app:cardCornerRadius="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cardCall"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardWeb"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="70dp"
        android:clickable="true"
        android:foreground="@drawable/ic_baseline_webbrowser_24"
        app:cardBackgroundColor="#344955"
        app:cardCornerRadius="16dp"
        app:cardElevation="10dp"
        app:layout_constraintEnd_toStartOf="@+id/cardClock"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="@+id/cardCall"
        app:layout_constraintTop_toBottomOf="@+id/cardCall">

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardClock"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="56dp"
        android:clickable="true"
        android:foreground="@drawable/ic_baseline_alarm_add_24"
        app:cardBackgroundColor="#344955"
        app:cardCornerRadius="16dp"
        app:cardElevation="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardMsg" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardCalendar"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="68dp"
        android:clickable="true"
        android:foreground="@drawable/ic_baseline_calendar_today_24"
        app:cardBackgroundColor="#344955"
        app:cardCornerRadius="16dp"
        app:cardElevation="10dp"
        app:layout_constraintStart_toStartOf="@+id/cardWeb"
        app:layout_constraintTop_toBottomOf="@+id/cardWeb">

    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>